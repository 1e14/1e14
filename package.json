  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:protoboard/river.git"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.16",
    "jasmine": "^3.3.1",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  },
  "scripts": {
    "build": "npm run build-river && npm run build-nodejs",
    "build-river": "cd modules/river && npm run build",
    "build-nodejs": "cd modules/river-nodejs && npm run build",
    "clean": "npm run clean-river && npm run clean-nodejs",
    "clean-river": "cd modules/river && npm run clean",
    "clean-nodejs": "cd modules/river-nodejs && npm run clean",
    "i-river": "cd modules/river && npm i",
    "i-nodejs": "cd modules/river-nodejs && npm i",
    "ln-river": "cd modules/river && npm run ln",
    "ln-nodejs": "cd modules/river-nodejs && npm run ln",
    "postinstall": "npm run postinstall-river && npm run postinstall-nodejs",
    "postinstall-river": "npm run ln-river && npm run i-river",
    "postinstall-nodejs": "npm run ln-nodejs && npm run i-nodejs",
    "test": "npm run test-river && npm run test-nodejs",
    "test-river": "cd modules/river && npm run test",
    "test-nodejs": "cd modules/river-nodejs && npm run test"
  }
}
