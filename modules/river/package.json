{
  "name": "@protoboard/river",
  "version": "0.1.0",
  "description": "Low-level, distributed computational graph library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "!dist/**/*.spec.*",
    "package.json",
    "README.md"
  ],
  "keywords": [
    "computational-graphs",
    "reactive-programming",
    "reactive-streams",
    "stream-processing",
    "distributed-computing",
    "fault-tolerance"
  ],
  "author": "Dan Stocker <dan@kwaia.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:protoboard/river.git"
  },
  "scripts": {
    "build": "npm run clean && tsc && echo river built",
    "clean": "rm -rf dist && echo river cleaned",
    "jasmine": "jasmine dist/**/*.spec.js && echo river tests passed",
    "ln": "npm ln && echo river linked",
    "test": "npm run tslint && npm run jasmine",
    "tslint": "tslint -c ../../tslint.json src/**/*.ts && echo river tslint passed"
  }
}
